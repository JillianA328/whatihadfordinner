{{#if posts.length}}
<section>
  <h2>Your Posts</h2>
    <!--show all posts from user-->
    {{#each posts as |post|}}

      {{> dashboard-posts post}}
    {{/each}}

</section>
{{/if}}

<button type="submit" class="btn"><a class="link" href="/dashboard/new">+ New Post</a></button>
<!--has button and connection to js file to add posts-->
<script src="/javascript/add-post.js"></script>